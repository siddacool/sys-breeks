                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
            </section>

        </div>
        <div box="fluid">
            <h5>Fluid</h5>
            <section>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
            </section>

        </div>
        <div box="justify">
            <h5>Justify</h5>
            <section>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
            </section>
        </div>
        <div box="expanded">
            <h5>expanded</h5>
            <section>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
                <div><span>Grid Normal</span></div>
            </section>
        </div>

        <!-- Headings -->
        <div box>
            <hr/>

            <h3>Headings</h3>
            <section style="margin-bottom: 3rem;">
                <div itm-s="100">
                    <h1>Heading</h1>
                    <h2>Heading</h2>
                    <h3>Heading</h3>
                    <h4>Heading</h4>
                    <h5>Heading</h5>
